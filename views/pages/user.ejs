<%- include('../partials/navbar') %> <%- include('../partials/sidebar') %>

<main class="content p-4" style="margin-left: 260px; margin-top: 60px">
	<h4 class="fw-semibold mb-4">Beneficiaries</h4>

	<div class="card p-3 shadow-sm">
		<div class="table-responsive">
			<table class="table table-hover mb-0 align-middle">
				<thead class="table-light">
					<tr>
						<th>SI</th>
						<th>Name</th>
						<th>Card Number</th>
						<th>Aadhar</th>
						<th>Members</th>
					</tr>
				</thead>
				<tbody>
					<% members.forEach((member, index) => { %>
					<tr>
						<td><%= index + 1 %></td>
						<td><%= member.name %></td>
						<td><%= member.cardNum || '-' %></td>
						<td><%= member.aadharNum || '-' %></td>
						<td><%= member.membersCount || '-' %></td>
					</tr>
					<% }) %>
				</tbody>
			</table>
		</div>
	</div>
</main>
